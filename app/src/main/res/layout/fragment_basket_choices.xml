<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorAccent"
    tools:context=".FragmentBasketChoices">

    <androidx.cardview.widget.CardView
        android:layout_width="350dp"
        android:layout_height="350dp"
        android:layout_marginBottom="208dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/buttonOrder"
                android:layout_width="106dp"
                android:layout_height="64dp"
                android:layout_marginStart="128dp"
                android:layout_marginEnd="128dp"
                android:layout_marginBottom="4dp"
                android:text="@string/order"
                app:cornerRadius="70dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="220dp"
                android:layout_height="54dp"
                android:layout_marginEnd="65dp"
                android:layout_marginBottom="6dp"
                app:layout_constraintBottom_toTopOf="@+id/radioButton"
                app:layout_constraintEnd_toEndOf="parent">

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="151dp"
                    android:layout_height="25dp"
                    android:layout_marginEnd="7dp"
                    android:layout_marginBottom="14dp"
                    android:text="@string/metoda_platnosci"
                    android:textAlignment="center"
                    android:textSize="16dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" />

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginBottom="11dp"
                    android:src="@android:drawable/btn_star_big_on"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout"
                android:layout_width="220dp"
                android:layout_height="54dp"
                android:layout_marginStart="65dp"
                android:layout_marginBottom="15dp"
                app:layout_constraintBottom_toTopOf="@+id/radioButton3"
                app:layout_constraintStart_toStartOf="parent">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginBottom="11dp"
                    android:src="@android:drawable/btn_star_big_on"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="151dp"
                    android:layout_height="25dp"
                    android:layout_marginEnd="6dp"
                    android:layout_marginBottom="14dp"
                    android:text="@string/dostawa"
                    android:textAlignment="center"
                    android:textSize="16dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <CheckBox
                android:id="@+id/radioButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="68dp"
                android:layout_marginBottom="4dp"
                android:text="@string/karta"
                app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
                app:layout_constraintStart_toStartOf="parent" />

            <CheckBox
                android:id="@+id/radioButton3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="64dp"
                android:layout_marginBottom="17dp"
                android:text="@string/adres"
                app:layout_constraintBottom_toTopOf="@+id/buttonOrder"
                app:layout_constraintStart_toStartOf="parent" />

            <CheckBox
                android:id="@+id/radioButton4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="84dp"
                android:layout_marginBottom="17dp"
                android:text="@string/sklep"
                app:layout_constraintBottom_toTopOf="@+id/buttonOrder"
                app:layout_constraintEnd_toEndOf="parent" />

            <CheckBox
                android:id="@+id/radioButton2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="64dp"
                android:layout_marginBottom="4dp"
                android:text="@string/przelew"
                app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
                app:layout_constraintEnd_toEndOf="parent" />

            <RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="39dp"
                tools:layout_editor_absoluteY="126dp" />

            <RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="157dp"
                tools:layout_editor_absoluteY="117dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>